<UIPlugin bundlePath="static">
  <View type="samPane">
    <Property name="singleton" value="true"/>
    <Property name="showMenu" value="true"/>
    <Property name="@menuName" value="samPane.name"/>

    <Import type="js" path="js/samPane.js"/>
    <Import type="js" path="js/SmoothStreamingPlayer.js"/>

    <Import type="css" path="css/samPane.css"/>

    <Property name="productId" value="samPane"/>
    <Property name="view-env" value="production"/>

  </View>
</UIPlugin>
